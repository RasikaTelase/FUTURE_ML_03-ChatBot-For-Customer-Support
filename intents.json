{
  "intents": [
    {
      "tag": "greeting",
      "patterns": ["Hi", "Hello", "Hey", "Good morning", "Good evening", "Howdy", "Hi there", "Hey there"],
      "responses": ["Hello! How can I help you today? 😊", "Hi there! Need any assistance?", "Hey! How’s it going?"]
    },
    {
      "tag": "goodbye",
      "patterns": ["Bye", "Goodbye", "See you later", "Catch you later", "See ya", "Talk to you later"],
      "responses": ["Goodbye! Have a great day! 👋", "See you soon! Take care!", "Bye! Come back anytime."]
    },
    {
      "tag": "thanks",
      "patterns": ["Thanks", "Thank you", "Thanks a lot", "Much appreciated", "Thx", "Thank you very much"],
      "responses": ["You’re welcome! 😄", "Happy to help!", "Anytime!"]
    },
